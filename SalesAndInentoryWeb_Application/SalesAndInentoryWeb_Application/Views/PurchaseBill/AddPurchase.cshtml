               "</td></div></tr>"
              tableitemlist.append(itemlist);
              reset();
          
              var finding = 0;
              $("tr").each(function (indexTr) {
               debugger
               if (isNaN(finding))
                  {
                      finding = 0;
                  }
                  var mm = +$(this).find('td:eq(7)').text();
                  finding = finding + mm;
                
                  $("#total1").val(finding);
