@model SalesAndInentoryWeb_Application.tbl_PartyGroup
@{
    Layout = null;
}

@using (Html.BeginForm())
{
    @Html.HiddenFor(model => model.PartyGroupID)
    <div class="form-group">
        @Html.LabelFor(model => model.AddPartyGroup, new { @class = "control-label" })
        @Html.EditorFor(model => model.AddPartyGroup, new { htmlAttributes = new { @class = "form-control", placeholder = "Enter Paryt Group....." } })
        @Html.ValidationMessageFor(model => model.AddPartyGroup)
    </div>
    <div class="form-group " style="text-align:center">
        <input type="submit" value="Submit" class="btn btn-primary " />
        <input type="reset" value="Reset" class="btn btn-info " style="margin-right:10px;" />
    </div>

}