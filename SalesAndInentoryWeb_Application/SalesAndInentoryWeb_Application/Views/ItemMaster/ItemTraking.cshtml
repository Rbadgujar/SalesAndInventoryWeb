@model SalesAndInentoryWeb_Application.Models.tbl_UnitMaster
@{
    Layout = null;
}

@*@using (Html.BeginForm("AddOrEdit", "BankAccount", FormMethod.Post, new { onsubmit = "return SubmitForm(this)" }))*@
@using(Html.BeginForm())
{
    @Html.HiddenFor(model => model.UnitName)
    <div class="form-group">
        @Html.LabelFor(model => model.UnitName, new { @class = "control-label" })
        @Html.EditorFor(model => model.UnitName, new { htmlAttributes = new { @class = "form-control", placeholder = "Enter Bank Name....." } })
        @Html.ValidationMessageFor(model => model.UnitName)
    </div>
    <div class="form-group">
        @Html.LabelFor(model => model.SubUnitName, new { @class = "control-label" })
        @Html.EditorFor(model => model.SubUnitName, new { htmlAttributes = new { @class = "form-control", placeholder = "Enter Account Name....." } })
        @Html.ValidationMessageFor(model => model.SubUnitName)
    </div>
   

    <div class="form-group footer">
        <input type="submit" value="Submit" class="btn btn-primary pull-right" />
        <input type="reset" value="Reset" class="btn btn-info pull-right" style="margin-right:10px;" />
    </div>
}