            $("#ts").change(function () {

                debugger
                var mrp = $("#sp").val();
                var tax = $("#ts").val();
                var dis = $("#ds").val();
                var qty = $("#qty").val();
                var itempricecall = mrp * qty;
                var tax_cal = itempricecall * tax / 100;
                var dis_count = itempricecall * dis / 100;
                var finalcal = (itempricecall + tax_cal) - dis_count

                $("#da").val(dis_count);
                $("#sa").val(tax_cal);
                $("#ca").val(finalcal);
                $("#qty").val(1);

            });

            $("#ds").change(function () {
                debugger
                var mrp = $("#sp").val();
                var tax = $("#ts").val();
                var dis = $("#ds").val();
                var qty = $("#qty").val();
                var itempricecall = mrp * qty;
                var tax_cal = itempricecall * tax / 100;
                var dis_count = itempricecall * dis / 100;
                var finalcal = (itempricecall + tax_cal) - dis_count

                $("#da").val(dis_count);
                $("#sa").val(tax_cal);
                $("#ca").val(finalcal);
                $("#qty").val(1);

            });
            $("#recived").change(function () {

                var recived = $("#recived").val();
                var pretotal = $("#Total").val();
                var tototo = pretotal - recived;
                $("#RemainingBal").val(tototo);

